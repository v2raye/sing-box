{
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      { "tag": "google", "address": "tls://8.8.8.8" },
      { "tag": "cloudflare", "address": "tls://1.1.1.1" },
      { "tag": "local", "address": "https://223.5.5.5/dns-query", "detour": "direct" },
      { "tag": "local-backup", "address": "https://dns.alidns.com/dns-query", "detour": "direct" },
      { "tag": "block", "address": "rcode://success" },
      { "tag": "remote", "address": "fakeip" }
    ],
    "rules": [
      { "outbound": "any", "server": "local" },
      { "rule_set": "geosite-cn", "server": "local" },
      { "rule_set": "geoip-cn", "server": "local" },
      { "rule_set": "geosite-!cn", "server": "google" }
    ],
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          { "port": 53 },
          { "protocol": "dns" }
        ],
        "outbound": "dns-out"
      },
      { "ip_is_private": true, "outbound": "direct" },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      { "domain": ["hxd.as174.com", "emby.as174.com"], "outbound": "Emby" },
      { "rule_set": "geosite-openai", "outbound": "OpenAI" },
      {
        "ip_cidr": ["149.154.160.0/20","91.108.4.0/22","91.108.8.0/22","2001:67c:4e8::/48","2001:b28:f23c::/48"],
        "outbound": "Europe"
      },
      {
        "ip_cidr": ["91.108.12.0/22","2001:b28:f23d::/48"],
        "outbound": "United States"
      },
      {
        "ip_cidr": ["91.108.16.0/22","91.108.56.0/22","2001:b28:f23f::/48"],
        "outbound": "Singapore"
      },
      { "rule_set": "geosite-netflix", "outbound": "NETFLIX" },
      { "rule_set": "geosite-apple", "outbound": "Apple" },
      { "rule_set": "geosite-microsoft", "outbound": "Microsoft" },
      { "rule_set": "geosite-!cn", "outbound": "Proxy" },
      { "rule_set": ["geosite-cn", "geoip-cn"], "outbound": "direct" }
    ],
    "auto_detect_interface": true,
    "final": "Proxy",
    "rule_set": [
      {
        "tag": "geosite-netflix",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-netflix.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-openai",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/v2raye/sing-box-rule/main/rule/ai.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-apple.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-microsoft",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-microsoft.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/v2raye/sing-box-rule/main/rule/cncidr.srs",
        "download_detour": "Proxy",
        "update_interval": "1d"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": ["Hong Kong","Taiwan","Japan","Singapore","United States","direct"]
    },
    {
      "tag": "Apple",
      "type": "selector",
      "outbounds": ["direct","Proxy","Hong Kong","Taiwan","Japan","Singapore","United States"]
    },
    {
      "tag": "NETFLIX",
      "type": "selector",
      "outbounds": ["Hong Kong","Taiwan","Japan","Singapore","United States"],
      "default": "Singapore"
    },
    {
      "tag": "Microsoft",
      "type": "selector",
      "outbounds": ["direct","Hong Kong","Taiwan","Japan","Singapore","United States"],
      "default": "direct"
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": ["Hong Kong","Taiwan","Japan","Singapore","United States"],
      "default": "United States"
    },
    {
      "tag": "Emby",
      "type": "selector",
      "outbounds": ["Enet"]
    },
    { "tag": "Hong Kong", "type": "selector", "outbounds": [] },
    { "tag": "Taiwan", "type": "selector", "outbounds": [] },
    { "tag": "Japan", "type": "selector", "outbounds": [] },
    { "tag": "Singapore", "type": "selector", "outbounds": [] },
    { "tag": "United States", "type": "selector", "outbounds": [] },
    { "tag": "Europe", "type": "selector", "outbounds": [] },
    { "tag": "Enet", "type": "selector", "outbounds": [] },
    { "tag": "direct", "type": "direct" },
    { "tag": "block", "type": "block" },
    { "tag": "dns-out", "type": "dns" }
  ],
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": ["172.19.0.1/30","fdfe:dcba:9876::1/126"],
      "stack": "mixed",
      "auto_route": true,
      "route_address": ["0.0.0.0/0","::/0"],
      "strict_route": true,
      "sniff": true,
      "sniff_override_destination": false,
      "mtu": 1400
    },
    {
      "type": "socks",
      "tag": "socks-in",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true
    },
    {
      "type": "http",
      "tag": "http-in",
      "listen": "127.0.0.1",
      "listen_port": 7891,
      "sniff": true
    }
  ],
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "Proxy",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  },
  "log": {
    "disabled": false,
    "level": "warn",
    "timestamp": true
  }
}